{
  "name": "aliexpres-scrapping",
  "version": "1.0.0",
  "description": "Microservicio Node.js para scrapear productos de AliExpress",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "install:browsers": "npx playwright install chromium",
    "docker:build": "docker build -t aliexpress-scraper .",
    "docker:run": "docker run -p 3001:3001 aliexpress-scraper",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "aliexpress",
    "scraper",
    "microservice",
    "express",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "openai": "^6.9.0",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^4.1.12"
  }
}
